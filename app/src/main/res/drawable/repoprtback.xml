<?xml version="1.0" encoding="utf-8"?>
<selector xmlns:android="http://schemas.android.com/apk/res/android" >
    <item android:state_pressed="true" >
        <shape android:shape="rectangle"  >
            <corners android:radius="20dp" />
            <stroke android:width="1dip" android:color="@color/googlepressed" />
            <gradient android:angle="-90" android:startColor="@color/googlepressed" android:endColor="@color/googlepressed"  />
        </shape>
    </item>
    <item android:state_focused="true">
        <shape android:shape="rectangle"  >
            <corners android:radius="20dp" />
            <stroke android:width="1dip" android:color="@color/googlepressed" />
            <solid android:color="@color/googlepressed"/>
        </shape>
    </item>
    <item >
        <shape android:shape="rectangle"  >
            <corners android:radius="20dp" />
            <stroke android:width="2dip"
                android:color="@color/googlepressed" />
            <gradient android:angle="-90"

                android:startColor="@color/color_garudauav_blue_line"
                android:endColor="@color/color_garudauav_blue_line" />
        </shape>
    </item>
</selector>